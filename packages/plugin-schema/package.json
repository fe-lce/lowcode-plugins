{
  "name": "@felce/lowcode-plugin-schema",
  "author": "alvarto",
  "version": "1.2.0",
  "description": "Show schema in lowcode editor",
  "files": [
    "dist"
  ],
  "main": "src/index.ts",
  "module": "src/index.ts",
  "README": "README.md",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build",
    "pub": "npm version patch && pnpm publish",
    "pub:major": "npm version major && pnpm publish",
    "pub:minor": "npm version minor && pnpm publish",
    "pub:patch": "npm version patch && pnpm publish",
    "pub:premajor": "npm version premajor --preid beta && pnpm publish --tag beta",
    "pub:preminor": "npm version preminor --preid beta && pnpm publish --tag beta",
    "pub:prepatch": "npm version prepatch --preid beta && pnpm publish --tag beta",
    "pub:prerelease": "npm version prerelease --preid beta && pnpm publish --tag beta"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "style": "dist/index.css",
    "exports": {
      ".": {
        "import": "./dist/index.es.js",
        "require": "./dist/index.umd.js",
        "style": "./dist/index.css"
      }
    }
  },
  "dependencies": {
    "@alifd/next": "^1.27.30",
    "@felce/lowcode-plugin-base-monaco-editor": "workspace:^",
    "classnames": "^2.5.1",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "vite": "^6.0.6",
    "@vitejs/plugin-react-swc": "3.7.1",
    "vite-plugin-dts": "^4.3.0",
    "@felce/lowcode-types": "^1.5.0-beta.4",
    "@types/react": "^18.3.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fe-lce/lowcode-plugins.git",
    "directory": "packages/plugin-schema"
  }
}
